#fungsi mencari luas
def luas(panjang,lebar):
    return panjang*lebar
#fungsi mencari keliling 
def keliling (panjang, lebar):
    return 2* panjang+lebar
a = int(input("masukkan nilai panjang: "))
b =int(input("masukkan nilai lebar: "))
hasil_fx =luas(a,b)
print("luas:",hasil_fx)
hasil_fx = keliling(a,b)
print("keliling:",hasil_fx)
